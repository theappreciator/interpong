{
  "author": "Jess Tucker <jessjesstheappreciator@gmail.com>",
  "license": "MIT",
  "name": "@interpong/client",
  "version": "1.0.0",
  "source": "public/index.html",
  "scripts": {
    "start": "npx http-server ./dist/",
    "start:dev": "parcel serve public/index.html",
    "watch": "parcel watch src/index.html",
    "build": "npm run clean && parcel build --dist-dir dist",
    "ci:build": "rm -fr ./dist && parcel build --dist-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist && npm run clean:parcel",
    "clean:parcel": "rm -rf .parcel-cache && rm -rf .parcel-cache.nosync && mkdir .parcel-cache.nosync && ln -s .parcel-cache.nosync .parcel-cache",
    "clean:modules": "rm -rf node_modules && rm -rf node_modules.nosync",
    "clean:package-lock": "rm -f package-lock.json",
    "create-no-sync": "npm run create-no-sync:node",
    "create-no-sync:node": "if [ -d node_modules ]; then rm -fr node_modules.nosync; mv node_modules node_modules.nosync; ln -s node_modules.nosync node_modules; fi"
  },
  "dependencies": {
    "@interpong/common": "^1.0.0",
    "@parcel/service-worker": "^2.8.3",
    "@types/node": "^18.11.18",
    "@types/node-sass": "^4.11.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.8.3",
    "pixi.js": "^7.2.3",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.61.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "typedi": "^0.10.0",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@types/babel__core": "^7.20.0",
    "@types/jest": "^29.5.1",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
